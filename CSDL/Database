CREATE TABLE CTHD(
    MAHD NCHAR(4) NOT NULL,
    MAHANG NCHAR(4) NOT NULL,
    SL INT
);
ALTER TABLE CTHD ADD CONSTRAINT PK_MAHANG PRIMARY KEY (MAHD, MAHANG)
CREATE TABLE HANGHOA(
	MAHANG NCHAR(4) NOT NULL PRIMARY KEY,
	TENHANG VARCHAR(50),
	GIA BIGINT
);
